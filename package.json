{
  "name": "sanity-plugin-documents-pane",
  "version": "1.0.8",
  "description": "Displays the results of a GROQ query in a View Pane",
  "main": "lib/index.js",
  "scripts": {
    "build": "sanipack build",
    "watch": "sanipack build --watch",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable && sanipack build && sanipack verify",
    "postpublish": "pinst --enable",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/sanity-plugin-documents-pane.git"
  },
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "dependencies": {
    "@sanity/base": "^2.29.5",
    "@sanity/desk-tool": "^2.29.5",
    "@sanity/state-router": "^2.29.3",
    "@sanity/ui": "^0.34.6",
    "dlv": "^1.1.3",
    "react-query": "^3.19.1",
    "husky": "^7.0.1"
  },
  "devDependencies": {
    "eslint": "7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-sanity": "5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "pinst": "^2.1.6",
    "prettier": "^2.3.2",
    "sanipack": "^2.1.0"
  },
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/sanity-plugin-document-pane/issues"
  },
  "homepage": "https://github.com/sanity-io/sanity-plugin-document-pane#readme",
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "parser": "sanipack/babel/eslint-parser",
    "extends": [
      "sanity",
      "sanity/react",
      "prettier"
    ]
  }
}

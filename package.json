  "peerDependencies": {
    "@sanity/util": "^2.29.5",
    "@sanity/uuid": "^3.0.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sanity": "dev-preview || 3.0.0-dev-preview.22"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@parcel/packager-ts": "^2.7.0",
    "@parcel/transformer-typescript-types": "^2.7.0",
    "@sanity/eslint-config-no-v2-imports": "^0.0.1-studio-v3.3",
    "@sanity/plugin-kit": "^1.0.2",
    "@sanity/semantic-release-preset": "^2.0.1",
    "@sanity/util": "^2.29.5",
    "@sanity/uuid": "^3.0.1",
    "@types/dlv": "^1.1.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "parcel": "^2.7.0",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sanipack": "^2.1.0",
    "sanity": "3.0.0-dev-preview.22",
    "typescript": "4.7.3"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/sanity-plugin-document-pane/issues"
